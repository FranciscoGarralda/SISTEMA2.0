{
  "name": "sistema-financiero-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:functions\"",
    "dev:functions": "netlify dev --functions netlify/functions --port 8888",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:full": "npm run test:setup && npm run test:integration",
    "test:setup": "node scripts/test-setup.js",
    "test:integration": "jest --testPathPattern=integration",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf .next out dist build coverage",
    "analyze": "ANALYZE=true npm run build",
    "health": "node scripts/health-check.js",
    "db:setup": "node scripts/setup-database.js",
    "db:seed": "node scripts/seed-database.js",
    "deploy": "npm run build && git add . && git commit -m \"ðŸš€ Deploy automÃ¡tico\" && git push origin main"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@netlify/functions": "^4.2.1",
    "@netlify/neon": "^0.1.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "next": "14.2.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.31",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^11.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "netlify-cli": "^17.0.0",
    "postcss": "^8",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.0"
  }
